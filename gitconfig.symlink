[core]
	quotepath = false
[alias]
  count  = !git shortlog -sn
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  co = checkout
  cl = clone
  br = branch
  s  = status
  st = status
  f  = fetch
  a  = add
  d  = diff
  ds = diff --staged
  pl = pull
  ps = push
[color]
  diff   = auto
  status = auto
  branch = auto
[core]
  excludesfile = ~/.gitignore
  editor = vim
[apply]
  whitespace = nowarn
